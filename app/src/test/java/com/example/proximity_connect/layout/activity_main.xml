<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/screenblack"
    android:orientation="vertical"
    android:paddingLeft="16dp"
    android:paddingRight="16dp">


    <EditText
        android:id="@+id/editTextText"
        android:layout_width="394dp"
        android:layout_height="wrap_content"
        android:background="@color/screenblack"
        android:ems="10"
        android:inputType="text"
        android:text="                            Proximity Connect"
        android:textColor="#FFFFFFFF" />


    <ImageButton
        android:layout_width="117dp"
        android:layout_height="82dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_marginStart="278dp"
        android:layout_marginTop="1dp"
        android:background="@drawable/settingsbutton" />

    <ImageButton
        android:layout_width="119dp"
        android:layout_height="78dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="1dp"
        android:background="@drawable/profile_button" />

    <ImageButton
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="619dp"
        android:background="@drawable/transcriptionbutton" />

    <ImageButton
        android:layout_width="147dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_marginStart="147dp"
        android:layout_marginTop="619dp"
        android:background="@drawable/home_button" />

    <ImageButton
        android:layout_width="98dp"
        android:layout_height="119dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_marginStart="275dp"
        android:layout_marginTop="619dp"
        android:background="@drawable/friends_tab" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="false"
        android:layout_marginStart="70dp"
        android:layout_marginLeft="66dp"
        android:layout_marginTop="250dp"
        android:background="@color/screenblack"
        android:ems="10"
        android:inputType="text"
        android:text="Start Meeting"
        android:textColor="#FFFFFFFF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_marginStart="79dp"
        android:layout_marginTop="309dp"
        android:text="Start" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_marginStart="250dp"
        android:layout_marginTop="253dp"
        android:background="@color/screenblack"
        android:ems="10"
        android:inputType="text"
        android:text="Join Meeting"
        android:textColor="#FFFFFFFF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_marginStart="258dp"
        android:layout_marginTop="310dp"
        android:text="Join" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="138dp"
        android:layout_height="280dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_marginStart="141dp"
        android:layout_marginTop="19dp"
        app:srcCompat="@drawable/proximity_logo" />

</RelativeLayout>

